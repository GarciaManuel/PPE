{
  "name": "jwt-mongodb-graphql",
  "version": "1.0.0",
  "description": "Sistema de autenticaci√≥n JWT con MongoDB en GraphQL",
  "main": "build/server.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
    "start:devW": "npm run build:devW",
    "build:devW": "nodemon src/server.ts --exec ts-node src/server.ts -e ts,graphql"
  },
  "keywords": [
    "graphql",
    "graphql-curso",
    "jwt",
    "mongodb"
  ],
  "author": "Anartz Mugika Ledo <mugan86@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-codegen": "^0.20.2",
    "apollo-server-express": "^2.6.7",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tools": "^4.0.5",
    "graphql-upload": "^10.0.0",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-graphql": "^0.8.0",
    "@types/graphql": "^14.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.0.10"
  }
}
